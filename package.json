{
  "name": "feimiao",
  "name_cn": "飞喵加速器",
  "name_en": "MH SPEED",
  "app_id": "com.flycat.fast",
  "version": "1.0.6",
  "version_force": false,
  "version_logs": [
    "更新支付方式",
    "更新优化线路",
    "更新UI展示"
  ],
  "updater_server": "http://47.83.183.124:8080",
  "description": "",
  "main": "index.js",
  "core_config": {
    "unpick_use_default": true,
    "use_last_choise": true
  },
  "scripts": {
    "obfuscate": "node build/obfuscate.js",
    "unobfuscate": "node build/unobfuscate.js",
    "channel": "node build/channel.js",
    "icons:tray": "cp ./icons/24x24.png ./icons/tray.png",
    "icons:window": "cp ./icons/24x24.png ./icons/window.png",
    "icons:win": "electron-icon-builder --input=./icons/win.png --output=./ --flatten && npm run icons:tray && npm run icons:window",
    "icons:mac": "electron-icon-builder --input=./icons/mac.png --output=./ --flatten && npm run icons:tray && npm run icons:window",
    "install:view:window": "cd ./views/window_src && npm install",
    "dev:view:window": "npm --prefix ./views/window_src run dev",
    "build:view:window": "npm --prefix ./views/window_src run build",
    "install:box": "npm install",
    "dev:box": "electron . LOG_TO_TERMINAL LOG_TO_FILE LOG_LEVEL=DEBUG PAGE_DEBUG PAGE_URL",
    "test:box": "electron . LOG_TO_TERMINAL LOG_LEVEL=DEBUG PAGE_DEBUG",
    "build:box:win": "electron-builder -p never",
    "build:box:mac": "electron-builder -p never",
    "pkg": "node build/package.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "25.1.1",
    "electron-builder": "^24.4.0",
    "electron-icon-builder": "^2.0.1",
    "javascript-obfuscator": "^4.1.1",
    "js-yaml": "^4.1.0",
    "xfuture": "2.9.9"
  },
  "dependencies": {
    "adm-zip": "^0.5.15",
    "axios": "^1.6.8",
    "crypto-js": "^4.2.0",
    "electron-asar-hot-updater": "^0.1.3",
    "form-data": "^4.0.1",
    "node-machine-id": "^1.1.12",
    "pino": "^9.5.0",
    "qs": "^6.12.1",
    "rotating-file-stream": "^3.2.5",
    "socks-proxy-agent": "^8.0.4",
    "tcp-ping": "^0.1.1",
    "uuid": "^9.0.1"
  },
  "build": {
    "appId": "com.flycat.fast",
    "productName": "飞喵加速器",
    "artifactName": "MH-SPEED-${os}-${arch}-${version}-setup.${ext}",
    "copyright": "Copyright © 2024 All Rights Reserved",
    "files": [
      "!*.src.js",
      "!build/**/*",
      "!libs/*.src.js",
      "!preloads/*.src.js",
      "!views/window_src/**/*",
      "!channels/**/*",
      "!server/**/*",
      "!node_modules/xfuture/**/*"
    ],
    "extraResources": [
      "node_modules/xfuture/index.js",
      "node_modules/xfuture/parser.js",
      "node_modules/xfuture/resources",
      "channel.json"
    ],
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false,
      "installerIcon": "icons/icon.ico",
      "installerHeaderIcon": "icons/icon.icoo"
    },
    "win": {
      "extraResources": [
        "node_modules/xfuture/package/${platform}-${arch}"
      ],
      "icon": "icons/icon.icns",
      "requestedExecutionLevel": "highestAvailable",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "dmg": {
      "sign": false,
      "icon": "icons/icon.icns",
      "background": "mac/dmg-bg.png",
      "contents": [
        {
          "x": 300,
          "y": 130,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 70,
          "y": 130,
          "type": "file"
        }
      ],
      "window": {
        "x": 400,
        "y": 200,
        "width": 405,
        "height": 300
      }
    },
    "mac": {
      "identity": null,
      "extraResources": [
        "node_modules/xfuture/package/${platform}"
      ],
      "icon": "icons/icon.icns",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "mac/entitlements.plist",
      "entitlementsInherit": "mac/entitlements.plist",
      "target": {
        "target": "dmg",
        "arch": [
          "x64",
          "arm64"
        ]
      }
    }
  }
}
